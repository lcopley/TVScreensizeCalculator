<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.stuffilike.tvscreensizecalculator" android:versionCode="18" android:versionName="JELLY_BEAN_MR2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/television_alt"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ScreenSize"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".HowToUse" android:noHistory="true"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.HOWTOUSE" />
                <category android:name="android.intent.category.DEFAULT"  />
            </intent-filter>
        </activity>

        <activity
            android:name=".Privacy"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.PRIVACY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

